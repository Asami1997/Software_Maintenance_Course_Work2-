<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="scene" prefHeight="500.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controllers.MainController">
   <children>
      <VBox prefHeight="680.0" prefWidth="640.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ToolBar id="toolbar" prefHeight="39.0" prefWidth="637.0" style="-fx-background-color: rgb(97,167,148);">
              <items>
                <Button fx:id="loadBtn" mnemonicParsing="false" styleClass="tool-btn" text="Load">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets></Button>
                  <Button fx:id="saveBtn" mnemonicParsing="false" styleClass="tool-btn" text="Save">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor></Button>
                  <Button fx:id="enlargeBtn" mnemonicParsing="false" styleClass="tool-btn" text="Enlarge">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor></Button>
                  <Button fx:id="shrinkBtn" mnemonicParsing="false" styleClass="tool-btn" text="Shrink">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor></Button>
                  <Label fx:id="viewLbl" text="100%" />
                  <Button fx:id="setBoatBtn" mnemonicParsing="false" styleClass="tool-btn" text="Set Boat">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor></Button>
                  <Button fx:id="setAxeBtn" mnemonicParsing="false" styleClass="tool-btn" text="Set Axe">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor></Button>
                  <Button fx:id="clearPosBtn" mnemonicParsing="false" styleClass="tool-btn" text="Clear Position">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor></Button>
              </items>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </ToolBar>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
               <children>
                  <ScrollPane fx:id="mapscroll" maxHeight="640.0" maxWidth="701.0" prefHeight="516.0" prefWidth="701.0" HBox.hgrow="ALWAYS">
                     <content>
                     
                     <StackPane fx:id="stackPane" alignment="CENTER" prefHeight="640.0" prefWidth="640.0">
                           <children>
                              <GridPane fx:id="mapviewer" onDragOver="#handleDragOver" prefHeight="700" prefWidth="700">
                                
                              </GridPane>
                           </children>
                       
                  </StackPane>
                        
                           
                     </content>
                  </ScrollPane>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
