<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.TilePane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="500.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controllers.MainController">
   <children>
      <VBox prefHeight="680.0" prefWidth="640.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ToolBar prefHeight="39.0" prefWidth="637.0">
              <items>
                <Button fx:id="loadBtn" mnemonicParsing="false" text="Load" />
                  <Button fx:id="saveBtn" mnemonicParsing="false" text="Save" />
                  <Button fx:id="enlargeBtn" mnemonicParsing="false" text="Enlarge" />
                  <Button fx:id="shrinkBtn" mnemonicParsing="false" text="Shrink" />
                  <Label fx:id="viewLbl" text="100%" />
                  <Button fx:id="setBoatBtn" mnemonicParsing="false" text="Set Boat" />
                  <Button fx:id="setAxeBtn" mnemonicParsing="false" text="Set Axe" />
                  <Button fx:id="clearPosBtn" mnemonicParsing="false" text="Clear Position" />
              </items>
            </ToolBar>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
               <children>
                  <ScrollPane fx:id="mapscroll" maxHeight="640.0" maxWidth="701.0" prefHeight="516.0" prefWidth="701.0" HBox.hgrow="ALWAYS">
                     <content>
                     
                     <StackPane fx:id = "stackPane" alignment="CENTER" prefHeight="640.0" prefWidth="640.0">
                     
                     <children>
                     
                      <TilePane fx:id="mapviewer" alignment="CENTER" prefHeight="640.0" prefWidth="640.0" onDragOver="#handleDragOver" >
                           <children>
                               
                           </children>
                           
                           </TilePane>
                         
                             </children>
                       
                  </StackPane>
                        
                           
                     </content>
                  </ScrollPane>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
